<div class="page-header">
  <h1>
    Client onboarding statuses

    <a href="/client_onboarding_statuses/new" class="btn btn-lg btn-success">New Client onboarding status</a>

  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>

        <th>Last edited by</th>

        <th>Notes</th>

        <th>Current impl status</th>

        <th>Certification date</th>

        <th>Kickoff date</th>

        <th>Client</th>

        <th>Actions</th>
      </tr>

      <% @client_onboarding_statuses.each do |client_onboarding_status| %>
      <tr>

        <td><%= client_onboarding_status.last_edited_by %></td>

        <td><%= client_onboarding_status.notes %></td>

        <td><%= client_onboarding_status.current_impl_status %></td>

        <td><%= client_onboarding_status.certification_date %></td>

        <td><%= client_onboarding_status.kickoff_date %></td>

        <td><%= client_onboarding_status.client_id %></td>

        <td>

          <a href="/client_onboarding_statuses/<%= client_onboarding_status.id %>" class="btn btn-primary">Show</a>
          <a href="/client_onboarding_statuses/<%= client_onboarding_status.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_client_onboarding_status/<%= client_onboarding_status.id %>" class="btn btn-danger" rel="nofollow">Delete</a>

        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
