<div class="page-header">
  <h1>
   Implementation Tracker

    <a href="/client_onboarding_statuses/new" class="btn btn-lg btn-success">Add Client</a>

  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>

        <th>Client</th>

        <th>Current impl status</th>

        <th>Kickoff date</th>

        <th>Certification date</th>

        <th>Actions</th>
      </tr>

      <% @client_onboarding_statuses.each do |status| %>
      <tr>

        <td><%= status.client.clientname %></td>

        <td><%= status.impl_status.status_name %></td>

        <td><%= status.kickoff_date %></td>

        <td><%= status.certification_date %></td>



        <td>

          <a href="/client_onboarding_statuses/<%= status.id %>" class="btn btn-primary">Show Client</a>
          <a href="/client_onboarding_statuses/<%= status.id %>/edit" class="btn btn-warning">Update Notes</a>

        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
