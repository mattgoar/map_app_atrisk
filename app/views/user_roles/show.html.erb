<div class="page-header">
  <h3>User Role #<%= @user_role.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">

      <dt>Role description</dt>
      <dd><%= @user_role.role_description %></dd>


      <dt>Role name</dt>
      <dd><%= @user_role.role_name %></dd>


    </dl>

    <div class="btn-group btn-group-justified">

      <a href="/user_roles" class="btn btn-primary">
        Back
      </a>
      <a href="/user_roles/<%= @user_role.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_user_role/<%= @user_role.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>

    </div>
  </div>
</div>
